@model juegoPD.Models.Jugador
<div class="row">
    <div class="col-lg-12 col-md-8 login-box" style="background-color:white">
        <div class="col-lg-12 login-title" style="color:black">
            GRACIAS POR JUGAR Y APRENDER
        </div>

        <div class="col-lg-12 login-form">
            <div class="col-lg-12 login-form">
                <h2>Estimado @Model.nombre @Model.apellido</h2>
                <span>Le agredecemos por jugar este juego.</span>
                <blockquote>
                    Porcentaje Total:  <kbd>@Model.puntuacionTotal %</kbd>
                </blockquote>
            </div>
        </div>
        <div class="col-lg-12 login-form">
            <div class="col-lg-12 login-btm login-button">
                <a href="/Home/Index" class="btn btn-outline-primary btn-block">GRACIAS</a>
            </div>
        </div>
    </div>
</div>
